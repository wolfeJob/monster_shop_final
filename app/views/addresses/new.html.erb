<%= form_tag "/users/:user_id/addresses", method: :post do %>
<%= label_tag :nickname, "Address type" %>
<%= text_field_tag :nickname %>

<%= label_tag :street, "Street Address" %>
<%= text_field_tag :street %>

<%= label_tag :city, "City" %>
<%= text_field_tag :city %>

<%= label_tag :state, "State"%>
<%= text_field_tag :state%>

<%= label_tag :zip, "Zip" %>
<%= text_field_tag :zip%>

<%= submit_tag "Create Address" %>
<% end  %>
